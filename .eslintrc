{
  "root": true,
  "parser": "@typescript-eslint/parser", // Specifies the ESLint parser
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "prettier",
    "prettier/standard",
    "prettier/@typescript-eslint",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/ban-types": 0
  }
}
